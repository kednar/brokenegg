          <argument>
            <kind>1</kind>
            <string>if hsp != 0
    {
    hsp -= (hdir*(1/timer)); // slow down egg horizontally in right direction, first 30 then 60
    }

if (vsp &lt; 3) vsp += grav;

if (hdir = -1 &amp;&amp; hsp &gt; 0) || (hdir = 1 &amp;&amp; hsp &lt; 0)
    {
    hsp = 0;
    }

// Vertical Collision
if (place_meeting(x,y+vsp,obj_collision))
    {
    if  vsp &gt;= breaking_point
        {
        break_egg = 1;
        }
    while (!place_meeting(x,y+sign(vsp),obj_collision))
        {
        y += sign(vsp);
        }
    vsp = 0;
    timer = 5; // slow down the egg horizontally when it hits bottom
    }

if (place_meeting(x,y+(sprite_height/2),obj_collision)) &amp;&amp; (break_egg = 1)
        {
        instance_create(x,y,obj_broken_egg);
        instance_destroy();
        }

// Horizontal Collision
if (place_meeting(x+hsp,y,obj_collision))
    {
    while (!place_meeting(x+sign(hsp),y,obj_collision))
        {
        x += sign(hsp);
        }
    hsp = 0;
    }
    
// Rotation
image_angle += (hsp * 5);

x += hsp;
y += vsp;
</string>
