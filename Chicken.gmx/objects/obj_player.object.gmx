        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Player Movement

// Get the player's input
key_right = keyboard_check(vk_right);
key_left = -keyboard_check(vk_left);
key_fly = keyboard_check(vk_up);
key_down = keyboard_check(vk_down);
key_layegg = keyboard_check_pressed(vk_space);

// Basic Movement
move = key_left + key_right;
hsp = move * movespeed;

// Flying
if (key_fly) &amp;&amp; (stamina &gt; 0)
    {
    flying = 1;
    if (vsp &gt; -flyspeed_max) vsp -= flyspeed;
    stamina -= stamina_loss;
    }
if flying = 0
    {
    if (stamina &lt; stamina_max) stamina += stamina_recovery;
    if (vsp &lt; 3) vsp += grav;
    }
flying = 0;
    
// Horizontal Movement
var hsp_final = hsp + hsp_carry;
hsp_carry = 0;

// Horizontal Collision
if (place_meeting(x+hsp_final,y,obj_collision))
    {
    while (!place_meeting(x+sign(hsp_final),y,obj_collision))
        {
        x += sign(hsp_final);
        }
    hsp_final = 0;
    hsp = 0;
    } 
x += hsp_final;

// Vertical Collision
if (place_meeting(x,y+vsp,obj_collision))
    {
    while (!place_meeting(x,y+sign(vsp),obj_collision))
        {
        y += sign(vsp);
        }
    vsp = 0;
    } 
y += vsp;

// Animate
if (move != 0)
    {
    image_xscale = move;
    }
if (place_meeting(x,y+1,obj_collision))
    {
    if (move != 0) sprite_index = spr_chicken_run; else sprite_index = spr_chicken_still;
    }
else
    {
    if (vsp &lt; 0) sprite_index = spr_chicken_fly; else sprite_index = spr_chicken_fall;
    }
    
/// Lay Eggs
if (key_layegg) &amp;&amp; (egg_counter &gt;= egg_delay)
    {
    instance_create(x-((global.hdir)*8),y-5,obj_egg);
    egg_delay = 120;
    egg_counter = 0;
    }
egg_counter += 1;
</string>
          </argument>
