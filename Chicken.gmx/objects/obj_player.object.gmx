        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Player Movement

// Get the player's input
key_right = keyboard_check(vk_right);
key_left = -keyboard_check(vk_left);
key_jump = keyboard_check_pressed(vk_space);
key_down = keyboard_check(vk_down);
key_layegg = keyboard_check_pressed(ord("X"));

// Basic Movement
move = key_left + key_right;
hdir = move;
hsp = move * movespeed;
if (vsp &lt; 10) vsp += grav;

// Jumping
if (place_meeting(x,y+1,obj_collision)) 
    {
    jumps = jumpsmax;
    }

if (key_jump) &amp;&amp; (jumps &gt; 0)
    {
    vsp = -jumpspeed;
    jumps -= 1;
    }
    
// Wall Jump
if (key_jump) &amp;&amp; (place_meeting(x+1,y,obj_collision) || place_meeting(x-1,y,obj_collision))
    {
    vsp = -jumpspeed;
    }
    
// Horizontal Movement
var hsp_final = hsp + hsp_carry;
hsp_carry = 0;

// Horizontal Collision
if (place_meeting(x+hsp_final,y,obj_collision))
    {
    while (!place_meeting(x+sign(hsp_final),y,obj_collision))
        {
        x += sign(hsp_final);
        }
    hsp_final = 0;
    hsp = 0;
    } 
x += hsp_final;

// Vertical Collision
if (place_meeting(x,y+vsp,obj_collision))
    {
    while (!place_meeting(x,y+sign(vsp),obj_collision))
        {
        y += sign(vsp);
        }
    vsp = 0;
    } 
y += vsp;

// Animate
if (move != 0)
    {
    image_xscale = move;
    }
if (place_meeting(x,y+1,obj_collision))
    {
    if (move != 0) sprite_index = spr_player_run; else sprite_index = spr_player;
    }
else
    {
    if (vsp &lt; 0) sprite_index = spr_player_jump; else sprite_index = spr_player_fall;
    }
    
/// Lay Eggs
if key_layegg
    {
    shoot_direction = point_direction(x,y,x+(hdir*(15)),y-15);
    instance_create(x,y,obj_egg);
    }
</string>
